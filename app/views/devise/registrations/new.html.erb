<p><%= devise_error_messages! %></p>

<div id="banner-wrapper" style="margin-top:-30px;">
 <div id="registro" class="box">
  <div class= "h1registro">
  <h2>Registrate</h2>
</div>

<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>

  <%= f.text_field :name, :autofocus => true, :class => 'inputs normal',:placeholder => "Nombre" %>

  <%= f.email_field :email, :class => 'inputs normal',:placeholder => "Correo Electr칩nico"  %>

 <!-- <%= f.collection_select(:college_id, College.all, :id, :name ) %> -->

  <p class="titulos-chicos">Define una contrase침a</p>

  <%= f.password_field :password, :class => 'inputs normal',:placeholder => "Contrase침a" %>

  <%= f.password_field :password_confirmation, :class => 'inputs normal',:placeholder => "Confirmar Contrase침a" %>

  <%= f.hidden_field :uid,:class => 'inputs normal', :placeholder=>"uid" %>

  <%= f.hidden_field :provider,:class => 'inputs normal', :placeholder=>"facebook" %>

  <%= f.hidden_field :user_profile_id, :value => 2 %>
<br/><br/>
<fieldset id="actions" style="margin-top: 20px;">
  <%= f.submit "Registrarme",:class => 'loginb', :style => 'position: relative; top: -61px;'  %>
<% end %>
<div class="row">
  <%= render "devise/shared/links" %>
</div>
</fieldset>
</div>
</div>
