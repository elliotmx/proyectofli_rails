<h1>Listing pdf_previews</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @pdf_previews.each do |pdf_preview| %>
  <tr>
    <td><%= pdf_preview.project_title %></td>
    <td><%= pdf_preview.problem %></td>
    <td><%= link_to 'Show', pdf_preview %></td>
    <td><%= link_to 'Edit', edit_pdf_preview_path(pdf_preview) %></td>
    <td><%= link_to 'Destroy', pdf_preview, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Pdf preview', new_pdf_preview_path %>
