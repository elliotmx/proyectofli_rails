<%- if controller_name != 'sessions' %>
  <%= button_to "Entrar", new_session_path(resource_name),:class => 'button loginb', :style => 'position: relative; left: 265px; top: -15px;'  %>
<% end -%>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= button_to "Registrate", new_registration_path(resource_name), :method => "get",:class => 'button loginb',:style => 'left: 238px;
position: relative; top: -15px;'  %>
<% end -%>

<%- if devise_mapping.recoverable? && controller_name != 'passwords' %>
  <%= link_to "¿Has olvidado tu contraseña?", new_password_path(resource_name), :style => 'margin-top: -137px; margin-right: 10px;' %><br />
<% end -%>

<%- if devise_mapping.confirmable? && controller_name != 'confirmations' %>
  <%= link_to "Didn't receive confirmation instructions?", new_confirmation_path(resource_name) %><br />
<% end -%>

<%- if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != 'unlocks' %>
  <%= link_to "Didn't receive unlock instructions?", new_unlock_path(resource_name) %><br />
<% end -%>

<%- if devise_mapping.omniauthable? %>
  <%- resource_class.omniauth_providers.each do |provider| %>
    <%= link_to image_tag("entraFace.png", :border => 0), omniauth_authorize_path(resource_name, provider), :style => 'top: -128px;
right: 3px; position: relative;' %><br />
  <% end -%>
<% end -%>